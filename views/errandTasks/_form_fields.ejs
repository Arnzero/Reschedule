
    <fieldset>
    <legend>Errand Header</legend>
        <div>
            <label for="miscName">Misc. Errands</label>
            <input type="text"
                name="miscName"
                id="miscName"
                value="miscname">
        </div>
        <div>
            <label for="priority">Priority: 1 for front, 0 for last</label>
            <input type="number"
                name="priority"
                id="priority"
                value=""
                min="0"
                max="200">
        </div>
    </fieldset>
    <fieldset>
    <legend>Errand Time Info</legend>
        <div>
            <label for="timePeriod20">20 Mins</label>
            <input id="timePeriod20"
                type="radio"
                name="timePeriod"
                value="20">
            <label for="timePeriod30">30 Mins</label>
            <input id="timePeriod30"
                type="radio"
                name="timePeriod"
                value="30">
            <label for="timePeriod40">40 Mins</label>
            <input id="timePeriod40"
                type="radio"
                name="timePeriod"
                value="40">
            <label for="timePeriod1">1 Hour</label>
            <input id="timePeriod1"
                type="radio"
                name="timePeriod"
                value="1">
            <label for="timePeriod1.5">1.5 Hours</label>
            <input id="timePeriod1.5"
                type="radio"
                name="timePeriod"
                value="90">
            <label for="timePeriod2">2 Hours</label>
            <input id="timePeriod2"
                type="radio"
                name="timePeriod"
                value="120">
            <label for="timePeriod2.5">2.5 Hours</label>
            <input id="timePeriod2.5"
                type="radio"
                name="timePeriod"
                value="150">
            <label for="timePeriod3">3 Hours</label>
            <input id="timePeriod3"
                type="radio"
                name="timePeriod"
                value="180">
            <label for="timePeriod3.5">3.5 Hours</label>
            <input id="timePeriod3.5"
                type="radio"
                name="timePeriod"
                value="210">   
            <label for="timePeriod4">4 Hours</label>
            <input id="timePeriod4"
                type="radio"
                name="timePeriod"
                value="240">
            <label for="timePeriod4.5">4.5 Hours</label>
            <input id="timePeriod4.5"
                type="radio"
                name="timePeriod"
                value="270">
            <label for="timePeriod5">5 Hours</label>
            <input id="timePeriod5"
                type="radio"
                name="timePeriod"
                value="300">             
        </div>
    </fieldset>
        <div>
            <!--boolean: if done, add to history record-->
            <input type="hidden"
            name="completed"
            value="false">
        </div>
    <br>


